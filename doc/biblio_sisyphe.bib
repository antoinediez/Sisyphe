@article{de_bortoli_quantitative_2020,
	abstract = {In this paper, we investigate the limiting behavior of a continuous-time counterpart of the Stochastic Gradient Descent (SGD) algorithm applied to two-layer overparameterized neural networks, as the number or neurons (ie, the size of the hidden layer) \$N {\textbackslash}to +{\textbackslash}infty\$. Following a probabilistic approach, we show 'propagation of chaos' for the particle system defined by this continuous-time dynamics under different scenarios, indicating that the statistical interaction between the particles asymptotically vanishes. In particular, we establish quantitative convergence with respect to \$N\$ of any particle to a solution of a mean-field McKean-Vlasov equation in the metric space endowed with the Wasserstein distance. In comparison to previous works on the subject, we consider settings in which the sequence of stepsizes in SGD can potentially depend on the number of neurons and the iterations. We then identify two regimes under which different mean-field limits are obtained, one of them corresponding to an implicitly regularized version of the minimization problem at hand. We perform various experiments on real datasets to validate our theoretical results, assessing the existence of these two regimes on classification problems and illustrating our convergence results.},
	author = {De Bortoli, V. and Durmus, A. and Fontaine, X. and Simsekli, U.},
	file = {arXiv Fulltext PDF:/Users/AntoineDiez/Zotero/storage/JHZXN4FD/De Bortoli et al. - 2020 - Quantitative Propagation of Chaos for SGD in Wide .pdf:application/pdf;arXiv.org Snapshot:/Users/AntoineDiez/Zotero/storage/9RUGH93Z/2007.html:text/html;2007.06352.pdf:/Users/AntoineDiez/Zotero/storage/PRU6LDEH/2007.06352.pdf:application/pdf},
	journal = {arXiv preprint: arXiv:2007.06352},
	keywords = {Mathematics - Probability, Statistics - Machine Learning, Computer Science - Machine Learning},
	title = {Quantitative {Propagation} of {Chaos} for {SGD} in {Wide} {Neural} {Networks}},
	url = {http://arxiv.org/abs/2007.06352},
	year = {2020},
	bdsk-url-1 = {http://arxiv.org/abs/2007.06352}}

@article{rotskoff_trainability_2019,
	abstract = {Neural networks, a central tool in machine learning, have demonstrated remarkable, high fidelity performance on image recognition and classification tasks. These successes evince an ability to accurately represent high dimensional functions, but rigorous results about the approximation error of neural networks after training are few. Here we establish conditions for global convergence of the standard optimization algorithm used in machine learning applications, stochastic gradient descent (SGD), and quantify the scaling of its error with the size of the network. This is done by reinterpreting SGD as the evolution of a particle system with interactions governed by a potential related to the objective or "loss" function used to train the network. We show that, when the number \$n\$ of units is large, the empirical distribution of the particles descends on a convex landscape towards the global minimum at a rate independent of \$n\$, with a resulting approximation error that universally scales as \$O(n{\textasciicircum}\{-1\})\$. These properties are established in the form of a Law of Large Numbers and a Central Limit Theorem for the empirical distribution. Our analysis also quantifies the scale and nature of the noise introduced by SGD and provides guidelines for the step size and batch size to use when training a neural network. We illustrate our findings on examples in which we train neural networks to learn the energy function of the continuous 3-spin model on the sphere. The approximation error scales as our analysis predicts in as high a dimension as \$d=25\$.},
	author = {Rotskoff, G. M. and Vanden-Eijnden, E.},
	file = {arXiv Fulltext PDF:/Users/AntoineDiez/Zotero/storage/D9D8PI2E/Rotskoff and Vanden-Eijnden - 2019 - Trainability and Accuracy of Neural Networks An I.pdf:application/pdf;arXiv.org Snapshot:/Users/AntoineDiez/Zotero/storage/W7WCNUPH/1805.html:text/html},
	journal = {arXiv preprint: arXiv:1805.00915},
	keywords = {Statistics - Machine Learning, Computer Science - Machine Learning, Condensed Matter - Statistical Mechanics},
	shorttitle = {Trainability and {Accuracy} of {Neural} {Networks}},
	title = {Trainability and {Accuracy} of {Neural} {Networks}: {An} {Interacting} {Particle} {System} {Approach}},
	url = {http://arxiv.org/abs/1805.00915},
	year = {2019},
	bdsk-url-1 = {http://arxiv.org/abs/1805.00915}}

